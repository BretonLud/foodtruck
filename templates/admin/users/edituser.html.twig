{% extends 'base.html.twig' %}

{% block title %}Modifier l'utilisateur{% endblock %}

{% block body %}
    
    <div class="form">
    <h1>Modifier l'utilisateur</h1>
    {{ form_start(userForm) }}
        {{ form_row(userForm.email) }}
        <div class="check">
        {{ form_row(userForm.roles) }}
        </div>
        {{ form_row(userForm.Valider) }}
        {{ form_row(userForm.Supprimer) }}
    {{ form_end(userForm) }}
    </div>
{% endblock %}